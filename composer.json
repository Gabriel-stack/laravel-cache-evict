{
    "name": "vectorial1024/laravel-cache-evict",
    "description": "Efficiently remove expired Laravel cache data",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vectorial1024\\LaravelCacheEvict\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vectorial1024"
        }
    ],
    "require": {
        "php": "~8.1",
        "illuminate/support": "^10.0|^11.0",
        "wilderborn/partyline": "^1.0",
        "ramazancetinkaya/byte-formatter": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit test"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vectorial1024\\LaravelCacheEvict\\CacheEvictServiceProvider"
            ]
        }
    }
}
